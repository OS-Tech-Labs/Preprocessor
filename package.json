{
  "name": "data-preprocessor",
  "displayName": "Data Preprocessor",
  "publisher": "OSTech-Labs",
  "description": "",
  "repository": "https://github.com/OS-Tech-Labs/Preprocessor",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand: dataCleaningAssistant.start",
    "onCommand: dataCleaningAssistant.deactivate"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
